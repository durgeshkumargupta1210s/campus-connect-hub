{
  "info": {
    "name": "CampusConnect API",
    "description": "API collection for CampusConnect backend",
    "version": "1.0.0"
  },
  "requests": [
    {
      "id": 1,
      "name": "Health Check",
      "method": "GET",
      "url": "http://localhost:5000/api/health"
    },
    {
      "id": 2,
      "name": "Register User",
      "method": "POST",
      "url": "http://localhost:5000/api/auth/register",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "name": "John Doe",
        "email": "john@example.com",
        "password": "password123",
        "role": "student"
      }
    },
    {
      "id": 3,
      "name": "Login",
      "method": "POST",
      "url": "http://localhost:5000/api/auth/login",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "email": "john@example.com",
        "password": "password123"
      }
    },
    {
      "id": 4,
      "name": "Get Profile",
      "method": "GET",
      "url": "http://localhost:5000/api/users/profile",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE"
      }
    },
    {
      "id": 5,
      "name": "Update Profile",
      "method": "PUT",
      "url": "http://localhost:5000/api/users/profile",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE",
        "Content-Type": "application/json"
      },
      "body": {
        "name": "Jane Doe",
        "phone": "9876543210",
        "college": "XYZ College",
        "branch": "Computer Science",
        "semester": "6",
        "bio": "Interested in web development",
        "skills": ["JavaScript", "React", "Node.js"],
        "interests": ["Tech", "Sports"]
      }
    },
    {
      "id": 6,
      "name": "Get All Events",
      "method": "GET",
      "url": "http://localhost:5000/api/events?page=1&limit=10"
    },
    {
      "id": 7,
      "name": "Create Event",
      "method": "POST",
      "url": "http://localhost:5000/api/events",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE",
        "Content-Type": "application/json"
      },
      "body": {
        "title": "Hackathon 2024",
        "description": "Annual hackathon event",
        "category": "hackathon",
        "date": "2024-03-15T10:00:00Z",
        "time": "10:00 AM",
        "location": "Main Campus",
        "capacity": 100,
        "organizer": "CLUB_ID"
      }
    },
    {
      "id": 8,
      "name": "Get All Clubs",
      "method": "GET",
      "url": "http://localhost:5000/api/clubs?page=1&limit=10"
    },
    {
      "id": 9,
      "name": "Create Club",
      "method": "POST",
      "url": "http://localhost:5000/api/clubs",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE",
        "Content-Type": "application/json"
      },
      "body": {
        "name": "Tech Club",
        "description": "For tech enthusiasts",
        "category": "technical",
        "email": "techclub@college.edu",
        "location": "Building A"
      }
    },
    {
      "id": 10,
      "name": "Join Club",
      "method": "POST",
      "url": "http://localhost:5000/api/clubs/CLUB_ID/join",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE"
      }
    },
    {
      "id": 11,
      "name": "Get Opportunities",
      "method": "GET",
      "url": "http://localhost:5000/api/opportunities?type=internship&page=1&limit=10"
    },
    {
      "id": 12,
      "name": "Apply for Opportunity",
      "method": "POST",
      "url": "http://localhost:5000/api/opportunities/OPPORTUNITY_ID/apply",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE"
      }
    },
    {
      "id": 13,
      "name": "Register for Event",
      "method": "POST",
      "url": "http://localhost:5000/api/registrations",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE",
        "Content-Type": "application/json"
      },
      "body": {
        "eventId": "EVENT_ID"
      }
    },
    {
      "id": 14,
      "name": "Get User Registrations",
      "method": "GET",
      "url": "http://localhost:5000/api/registrations?page=1&limit=10",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE"
      }
    },
    {
      "id": 15,
      "name": "Get Tickets",
      "method": "GET",
      "url": "http://localhost:5000/api/tickets",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE"
      }
    },
    {
      "id": 16,
      "name": "Create Payment",
      "method": "POST",
      "url": "http://localhost:5000/api/payments",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE",
        "Content-Type": "application/json"
      },
      "body": {
        "amount": 500,
        "paymentMethod": "upi",
        "description": "Event ticket payment",
        "relatedTo": "ticket",
        "relatedId": "TICKET_ID"
      }
    },
    {
      "id": 17,
      "name": "Get Campus Drives",
      "method": "GET",
      "url": "http://localhost:5000/api/campus-drives?page=1&limit=10"
    },
    {
      "id": 18,
      "name": "Register for Campus Drive",
      "method": "POST",
      "url": "http://localhost:5000/api/campus-drives/DRIVE_ID/register",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE"
      }
    }
  ]
}
